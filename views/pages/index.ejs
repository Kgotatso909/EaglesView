    <style>
      /* Hero section starts here */

      .hero {
        position: relative;
        display: flex;
        justify-content: center;
        align-items: center;
        background-image: url("assets/images/hero-lg-screen.webp");
        background-size: cover;
        background-repeat: no-repeat;
        background-attachment: fixed;
        background-position: center;
        min-height: 100dvh;
        padding: 100px 20px;
        color: #fff;
        text-shadow: 1px 1px 3px rgba(0, 0, 0, 0.7);
      }

      .hero-bg-color {
        position: absolute;
        top: 0;
        left: 0;
        content: "";
        width: 100%;
        height: 100%;
        background-color: #00000085;
      }

      .hero-content {
        z-index: 1;
      }
      .hero h1 {
        font-size: 2.5rem;
        margin-bottom: 20px;
        font-family: "Broadway";
        text-shadow: 2px 2px 3px #00000086;
        font-weight: 400;
        font-style: normal;
        color: #fff;
        text-transform: capitalize;
      }

      .hero p {
        font-family: "Poppins", sans-serif;
        font-weight: 400;
        font-style: normal;
        font-size: 1.5rem;
        margin-bottom: 30px;
        color: #fff;
      }

      /* Media query for changing hero background image for 880px screen */
      @media (max-width: 880px) {
        .hero {
          position: relative;
          display: flex;
          justify-content: center;
          align-items: center;
          background-image: url("assets/images/hero-sm-screen-1.webp") !important;
          background-size: cover;
          background-repeat: no-repeat;
          background-attachment: fixed;
          background-position: center;
          min-height: 100dvh;
        }
      }

      @media (max-width: 768px) {
        .hero h1 {
          font-size: 1.8rem;
          text-align: center;
        }

        .hero p {
          font-size: 1.2rem;
        }
      }

      @media (max-width: 480px) {
        .hero h1 {
          font-size: 1.5rem;
          text-align: center;
        }

        .hero p {
          font-size: 1rem;
        }
      }

      @media (max-width: 991px) {
        .navbar-brand img {
          height: 70px !important;
          width: 70px !important;
        }
      }

      @media (min-width: 992px) {
        .navbar {
          background-color: transparent !important;
          transition: background-color 0.3s ease;
          color: #352d2d !important;
          box-shadow: none !important;
        }

        .navbar .nav-link {
          color: #fff !important;
        }

        .navbar.scrolled {
          background-color: #ebe8d8 !important;
          box-shadow: 2px 2px 3px rgba(0, 0, 0, 0.1) !important;
        }

        .navbar-brand {
          color: #fff !important;
        }

        .navbar.scrolled .navbar-brand img {
          height: 90px !important;
          width: 90px !important;
        }

        .navbar.scrolled .nav-link {
          color: #000 !important;
        }
      }
    </style>
